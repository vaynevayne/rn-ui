{
  "name": "@repo/ui",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "source": "src/index.ts",
  "module": "dist/esm/index.mjs",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "rollup --config node:@repo/rollup-config --watch",
    "build": "concurrently \"rollup --config node:@repo/rollup-config --environment NODE_ENV:production\" pnpm:tsc",
    "clean": "rimraf dist .turbo",
    "tsc": "tsc -p tsconfig.json --declaration true --emitDeclarationOnly --declarationDir dist/types",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "typecheck": "tsc --noEmit",
    "generate:component": "turbo gen react-component"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/rollup-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@tsconfig/react-native": "^3.0.5",
    "@turbo/gen": "^1.12.4",
    "@types/eslint": "^8.56.5",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "classnames": "^2.5.1",
    "eslint": "^8.57.0",
    "nativewind": "4",
    "react": "^18.2.0",
    "react-native": "0.73.6",
    "react-native-reanimated": "^3.8.1",
    "react-native-root-siblings": "^5.0.1",
    "react-native-svg": "^15.1.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.3.3",
    "zustand": "^4.5.2"
  },
  "peerDependencies": {
    "classnames": "^2.5.1",
    "nativewind": "4",
    "react": "^18.2.0",
    "react-native": "0.73.6",
    "react-native-reanimated": "^3.8.1",
    "react-native-root-siblings": "^5.0.1",
    "react-native-svg": "^15.1.0",
    "tailwindcss": "^3.4.3",
    "zustand": "^4.5.2"
  },
  "dependencies": {
    "@repo/hooks": "workspace:^"
  },
  "files": [
    "dist"
  ]
}
